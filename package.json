{
  "name": "skarb-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "format:write": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.{ts,tsx}",
    "bundlewatch": "bundlewatch --config bundlewatch.config.js",
    "db:pull:local": "dotenv -e .env.local -- npx prisma db pull",
    "db:migrate:local": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env.prod -- npx prisma migrate deploy",
    "db:push:local": "dotenv -e .env.local -- npx prisma db push"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.2.0",
    "@hookform/resolvers": "^4.1.1",
    "@prisma/client": "^5.15.0",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-table": "^8.21.3",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "next": "^14.2.18",
    "next-auth": "^5.0.0-beta.19",
    "prisma": "^5.15.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.13.0-alpha.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "babel-plugin-explicit-exports-references": "^1.0.2",
    "bundlewatch": "^0.4.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
