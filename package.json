{
  "name": "skarb-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "check-format": "prettier --check .",
    "db:pull:local": "dotenv -e .env.local -- npx prisma db pull",
    "db:migrate:local": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env.prod -- npx prisma migrate deploy",
    "db:push:local": "dotenv -e .env.local -- npx prisma db push"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.2.0",
    "@prisma/client": "^5.15.0",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "next": "14.2.4",
    "next-auth": "^5.0.0-beta.19",
    "prisma": "^5.15.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.0",
    "recharts": "^2.13.0-alpha.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "nextBundleAnalysis": {
    "budget": null,
    "budgetPercentIncreaseRed": 20,
    "minimumChangeThreshold": 0,
    "showDetails": true
  }
}
